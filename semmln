java -Xmx2G -XX:+HeapDumpOnOutOfMemoryError -cp SemSRL/bin/:SemSRL/lib/* org.aksw.simba.semsrl.SemMLN publications
echo "Copying files to Alchemy..."
cp ~/PhD/srl/SemSRL/publications-alchemy/publications.* alchemy-2/exdata/
alchemy-2/bin/infer -maxSteps 500 -burnMaxSteps 500 -i "alchemy-2/exdata/publications.mln" -r "SemSRL/publications-alchemy/publications-out.mln" -e "alchemy-2/exdata/publications.db" -q EqualsEnt
java -Xmx2G -XX:+HeapDumpOnOutOfMemoryError -cp SemSRL/bin/:SemSRL/lib/* org.aksw.simba.semsrl.eval.AlchemyEvaluation publications

