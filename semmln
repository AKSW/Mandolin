java -Xmx2G -XX:+HeapDumpOnOutOfMemoryError -cp target/classes/:lib/* org.aksw.simba.semsrl.SemMLN publications alchemy
echo "Copying files to Alchemy..."
cp publications-alchemy/publications.* ../alchemy-2/exdata/
